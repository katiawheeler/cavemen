{
  "name": "cavemen",
  "version": "0.0.19",
  "author": "Katia Wheeler",
  "description": "Primitive react components",
  "license": "MIT",
  "repository": "https://github.com/katiawheeler/cavemen.git",
  "bugs": {
    "url": "https://github.com/katiawheeler/cavemen/issues"
  },
  "homepage": "https://github.com/katiawheeler/cavemen#readme",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "keywords": [
    "react",
    "dropdown",
    "select",
    "button",
    "ui",
    "primitives"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/lodash.unionby": "^4.8.6",
    "lodash.intersection": "^4.4.0",
    "lodash.unionby": "^4.8.0",
    "lodash.uniqueid": "^4.0.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "scripts": {
    "start": "start-storybook -p 9009",
    "check-types": "tsc",
    "build": "rimraf ./dist && yarn build:rollup",
    "build:rollup": "rollup -c rollup.config.js",
    "test": "react-scripts test --watch --collectCoverage",
    "test:debug": "react-scripts test --watch --collectCoverage --verbose=false",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.prod.json",
    "lint:fix": "tslint --fix -p tsconfig.prod.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "23.3.13",
    "@types/lodash.intersection": "^4.4.6",
    "@types/lodash.uniqueid": "^4.0.6",
    "@types/node": "10.12.21",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-minify": "^0.5.0",
    "babel-preset-minify": "^0.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest-dom": "^3.1.3",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "react-test-renderer": "^16.8.4",
    "react-testing-library": "^6.0.2",
    "rollup": "^1.7.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/components/index.ts",
      "!src/**/*.styles.ts",
      "!src/**/*.stories.js",
      "!src/setupTests.ts",
      "!src/index.ts"
    ]
  }
}
